{
    "$schema": "https://turborepo.org/schema.json",
    "baseBranch": "origin/master",
    "pipeline": {
        "build": {
          "dependsOn": ["^build"],
          "outputs": [".next/**"]
        },
        "test": {
          "dependsOn": ["^build"],
          "outputs": []
        },
        "test:build": {
          "outputs": []
        },
        "test:lint": {
          "outputs": []
        },
        "test:unit": {
          "outputs": []
        },
        "test:watch": {
          "outputs": []
        },
        "start": {
          "cache": false
        },
        "deploy": {
          "dependsOn": ["^build"]
        }
      }
}